
* disp (t) 
	(if 
		(is name (t)) (t) 
		(is text (t)) (join "\"" (join (escp (t)) "\""))
		(is numb (t)) (t) 
		(if (t) "(yes)" "(no)")
	)
* test (txt re) 
	(info "(mtch " (disp (txt)) " " (disp (re)) ") = " (disp (do (ptrn (re)) (mtch (txt)))))

