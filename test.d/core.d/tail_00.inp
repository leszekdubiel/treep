
* disp (t) 
	(if (is name (t))
		(t) 
		(join "\"" (join (escp (t)) "\""))
	)

* wytnij_tail (t n) 
	(info "(tail " (disp (t)) " " (n) ") = " (disp (tail (t) (n))))

