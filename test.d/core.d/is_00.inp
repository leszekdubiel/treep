
* testuj_typ (n v) 
	(info "zmienna " (v) " ma typ " (n) "? ====> " (is (n) (v)))

* testuj_wszystkie (v) 
	(do 
		(testuj_typ void (v))
		(testuj_typ spac (v))
		(testuj_typ tabu (v))
		(testuj_typ line (v))
		(testuj_typ lpar (v))
		(testuj_typ rpar (v))
		(testuj_typ numb (v))
		(testuj_typ name (v))
		(testuj_typ text (v))
		(testuj_typ bool (v))
		(testuj_typ node (v))
		(testuj_typ tree (v))
	)

