
* disp (t) 
	(if 
		(is name (t)) (t) 
		(is text (t)) (join "\"" (join (escp (t)) "\""))
		(if (t) "yes" "no")
	)

* czy_czesc (t p) 
	(info "(part " (disp (t)) " " (disp (p)) ") = " (part (t) (p)))

