* does_look_like (n v) 
	(as 
		j (like (n) (v))
		(if (j) 
			(as
				f (if 
					(same name (n)) (name (v))
					(same numb (n)) (numb (v))
					(same time (n)) (repr (pars (v)))
					(do (info "wrong name " (n)) (exit (no)))
				)
				(info "\"" (escp (v)) "\", " (n) " " (f))
			)
			(info "\"" (escp (v)) "\", NOT a " (n))
		)
	)

